{
  "rewrites": [
    {
      "source": "/main",
      "destination": "/main/index.html"
    },
    {
      "source": "/auth/sign-in/",
      "destination": "/auth/sign-in/index.html"
    },
    {
      "source": "/auth/sign-up/",
      "destination": "/auth/sign-up/index.html"
    },
    {
      "source": "*",
      "destination": "/index.html"
    }
  ]
}